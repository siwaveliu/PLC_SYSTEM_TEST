---
# example data for MME_DISCOVER_NODE_LIST frame
header:
    mmtype:          MME_DISCOVER_NODE_LIST
body:
    tei:                          0x4
    proxy_tei:                    0x2
    role:                         NML_ROLE_TYPE_STATION
    level:                        2 
    sta_mac_addr:                 BB-BB-BB-BB-BB-BB
    cco_mac_addr:                 00-00-00-00-00-9C
    phase_1:                      PHASE_A
    phase_2:                      PHASE_B
    phase_3:                      PHASE_C
    pco_chan_quality:             15
    pco_succ_rate:                100
    pco_dl_succ_rate:             100
    station_num:                  2
    sent_dnl_msg_num:             10
    ul_route_item_num:            1
    route_period_remaining_time:  25
    bitmap_size:                  1
    min_succ_rate:                15
    ul_route_item_list:
    -   route_type:               NML_PROXY_ROUTE_TYPE
        next_hop_tei:             2        
    bitmap:                       [0xC]
    recv_discover_node_list:      [10,10]